{
  "Database": {
    "ConnectionString": "Server=localhost;Port=3307;Database=deepsid;User=deepsid_user;Password=deepsid_pass;AllowZeroDateTime=True;ConvertZeroDateTime=True;",
    "CommandTimeout": 300
  },
  "Export": {
    "OutputDirectory": "export",
    "OutputFileName": "deepsid_db.json",
    "DeleteExistingFile": true,
    "ValidateOutput": true
  },
  "Transformations": {
    "HvscPathPrefix": "_High Voltage SID Collection",
    "NotableReplacement": {
      "From": "Created this web site",
      "To": "Creator of DeepSID"
    },
    "DefaultTagType": "OTHER"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning"
    }
  }
}