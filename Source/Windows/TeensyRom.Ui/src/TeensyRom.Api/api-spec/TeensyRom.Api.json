    "/devices/{deviceId}/storage/{storageType}/directories": {
      "get": {
        "tags": [
          "Files"
        ],
        "summary": "Get Directory",
        "description": "Gets a directory for given storage device.\n\n- Returns metadata for all files in the directory.\n- This is not recursive and will only include the files for the requested directory.\n- Make another request to get subdirectory content.",
        "operationId": "GetDirectory",
        "parameters": [
          {
            "name": "deviceId",
            "in": "path",
