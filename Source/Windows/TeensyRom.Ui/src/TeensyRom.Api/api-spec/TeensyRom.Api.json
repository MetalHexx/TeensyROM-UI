  },
  "components": {
    "schemas": {
      "CartDto": {
        "required": [
          "deviceId",
          "isConnected",
          "deviceState",
          "comPort",
          "name",
          "fwVersion",
          "isCompatible",
          "sdStorage",
          "usbStorage"
        ],
        "type": "object",
        "properties": {
          "deviceId": {
            "type": "string"
          },
          "isConnected": {
            "type": "boolean"
          },
          "deviceState": {
            "$ref": "#/components/schemas/DeviceState"
          },
          "comPort": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "fwVersion": {
            "type": "string"
          },
          "isCompatible": {
            "type": "boolean"
          },
          "sdStorage": {
            "$ref": "#/components/schemas/CartStorageDto"
          },
          "usbStorage": {
            "$ref": "#/components/schemas/CartStorageDto"
          }
        }
      },
      "CartStorageDto": {
        "required": [
          "deviceId",
          "type",
          "available"
        ],
        "type": "object",
        "properties": {
          "deviceId": {
            "type": "string"
          },
          "type": {
            "$ref": "#/components/schemas/TeensyStorageType"
          },
          "available": {
            "type": "boolean"
          }
        }
      },
      "ConnectDeviceResponse": {
        "required": [
          "connectedCart",
          "message"
        ],
        "type": "object",
        "properties": {
          "connectedCart": {
            "$ref": "#/components/schemas/CartDto"
          },
          "message": {
            "type": "string"
          }
        }
      },
      "DeviceStartResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        }
      },
      "DeviceState": {
        "enum": [
          "Connected",
          "Connectable",
          "ConnectionLost",
          "Busy",
          "Unknown"
        ]
      },
      "DeviceStopResponse": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        }
      },
      "DirectoryItemDto": {
        "required": [
          "name",
          "path"
        ],
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "path": {
            "type": "string"
          }
        }
      },
      "DisconnectDeviceResponse": {
        "required": [
          "message"
        ],
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        }
      },
      "FileItemDto": {
        "required": [
          "name",
          "path",
          "size",
          "isFavorite",
          "isCompatible",
          "title",
          "creator",
          "releaseInfo",
          "description",
          "shareUrl",
          "metadataSource",
          "meta1",
          "meta2",
          "metadataSourcePath",
          "parentPath",
          "playLength",
          "subtuneLengths",
          "startSubtuneNum",
          "images",
          "type"
        ],
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "path": {
            "type": "string"
          },
          "size": {
            "type": "integer",
            "format": "int64"
          },
          "isFavorite": {
            "type": "boolean"
          },
          "isCompatible": {
            "type": "boolean"
          },
          "title": {
            "type": "string"
          },
          "creator": {
            "type": "string"
          },
          "releaseInfo": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "shareUrl": {
            "type": "string"
          },
          "metadataSource": {
            "type": "string"
          },
          "meta1": {
            "type": "string"
          },
          "meta2": {
            "type": "string"
          },
          "metadataSourcePath": {
            "type": "string"
          },
          "parentPath": {
            "type": "string"
          },
          "playLength": {
            "pattern": "^-?(\\d+\\.)?\\d{2}:\\d{2}:\\d{2}(\\.\\d{1,7})?$",
            "type": "string"
          },
          "subtuneLengths": {
            "type": "array",
            "items": {
              "pattern": "^-?(\\d+\\.)?\\d{2}:\\d{2}:\\d{2}(\\.\\d{1,7})?$",
              "type": "string"
            }
          },
          "startSubtuneNum": {
            "type": "integer",
            "format": "int32"
          },
          "images": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/ViewableItemImageDto"
            }
          },
          "type": {
            "$ref": "#/components/schemas/FileItemType"
          }
        }
      },
      "FileItemType": {
        "enum": [
          "Unknown",
          "Song",
          "Game",
          "Image",
          "Hex"
        ]
      },
      "FindDevicesResponse": {
        "required": [
          "devices",
          "message"
        ],
        "type": "object",
        "properties": {
          "devices": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/CartDto"
            }
          },
          "message": {
            "type": "string"
          }
        }
      },
      "GetDirectoryResponse": {
        "required": [
          "storageItem",
          "message"
        ],
        "type": "object",
        "properties": {
          "storageItem": {
            "$ref": "#/components/schemas/StorageCacheDto"
