  "paths": {
    "/devices/{deviceId}/reset": {
      "put": {
        "tags": [
          "Devices"
        ],
        "summary": "Reset Device",
        "description": "Resets a TeensyROM device.\n\n- Works the same as clicking the cartridge reset button.",
        "operationId": "ResetDevice",
        "parameters": [
          {
            "name": "deviceId",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ResetDeviceResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/problem+json": {
                "schema": {
                  "$ref": "#/components/schemas/ProblemDetails"
                }
              }
            }
          }
        }
      }
    },
